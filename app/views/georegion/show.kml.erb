<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="https://www.opengis.net/kml/2.2" xmlns:gx="https://www.google.com/kml/ext/2.2" xmlns:kml="https://www.opengis.net/kml/2.2" xmlns:atom="https://www.w3.org/2005/Atom">
<Document>
  <name><%= @area.name.titleize.delete(' ').underscore %></name>
  <description></description>
  <% @projects_for_kml.each do |project| %>
    <Placemark>
      <name><%= project['project_name'] %></name>
      <%= raw(project['kml']) %>
    </Placemark>
  <% end %>
</Document>
</kml>
